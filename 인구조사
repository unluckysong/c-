#include <iostream>

int main(void)
{
    char name [100];
    char number [200];

    std::cout<<"당신의 이름은?"<<std::endl;
    std::cin>>name;

    std::cout<<"당신의 전화번호는?"<<std::endl;
    std::cin>>number;

    std::cout<<"당신의 이름은: "<<name<<std::endl;
    std::cout<<"당신의 전화번호는: "<<number<<std::endl;
    return 0;
}
